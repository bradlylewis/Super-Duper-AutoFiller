{
  "manifest_version": 3,
  "name": "Super Duper Auto-filler",
  "version": "1.2.0",
  "description": "Fills out job application forms automatically",
  "permissions": ["storage", "scripting", "activeTab"],
  "host_permissions": ["<all_urls>"],
  "background": {
    "service_worker": "background.js"
  },
  "action": {
    "default_popup": "popup.html",
    "default_icon": {
      "16": "icon.png",
      "48": "icon.png",
      "128": "icon.png"
    }
  },
  "options_page": "options.html"
}
